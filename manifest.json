{
    "manifest_version": 2,
  
    "name": "pkData",
    "description": "pkData Info",
    "version": "1.1",

    "web_accessible_resources": [
      "pk.html",
      "pk.js",
      "pk.css",
      "pk.json",
      "knockout-3.4.2.js",
      "jquery.min.js"
    ],
    "permissions": [
      "contextMenus",
      "tabs",
      "http://*/*",
      "https://*/*",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["pkBackground.js"]
    },
    "browser_action": {
      "default_popup": "pk.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }